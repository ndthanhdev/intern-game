{
  "welcomeFile": "/com/sap/thanh/app/todo/app/index.html",
  "authenticationMethod": "none",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/com/sap/thanh/app/todo/app/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    },
    {
      "source": "^/com/sap/thanh/app/todo/resources/(.*)$",
      "target": "$1",
      "localDir": "resources"
    },
    {
      "source": "^/com/sap/thanh/app/todo/bower_components/(.*)$",
      "target": "$1",
      "localDir": "bower_components"
    },
    {
      "source": "^/com/sap/thanh/app/todo/rest/(.*)$",
      "target": "$1",
      "destination": "backend"
    }
  ]
}